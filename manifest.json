{
	"manifest_version":2,
	"name":"ASCII circles",
	"description":"View a generated ASCII circle for the text on any webpage",
	"version":"1.0",
	"permissions":["tabs", "<all_urls>"],
	"content_scripts": [
	    {
	      "matches": ["<all_urls>"],
	      "js": ["contentscript.js"]
	    }
	  ],
	"browser_action":{
		"default_icon":"icon.png",
		"default_popup":"popup.html"
	}
}